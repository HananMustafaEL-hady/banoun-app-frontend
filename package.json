{
  "name": "banoun-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@popperjs/core": "^2.9.2",
